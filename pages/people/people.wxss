.swiper-tab{ height: 30px; line-height: 30px; display: flex; position: relative; z-index: 2; border-bottom: 1px solid #F4F4F4; flex-direction:row; justify-content:center; align-items:center;}
.swiper-tab-list{ margin: 0 10px;  padding: 0 4px; font-size: 28rpx;}
.on{ border-bottom: 1px solid #48C23D; color: #48C23D; }
.swiper-item {
  height: 300px;
  width: 100%;
  /* background: red; */
}
.themes-list{ background: #FFF; display: block; margin-bottom: 0px; }
.themes-list-box{ display: block; position: relative; padding: 2px; border-bottom: 1px solid #F2F2F2; }
.themes-list-main{ margin-left: 10px; }
.themes-list-name{ font-size: 14px; color: #444; height: 20px; line-height: 20px; overflow: hidden; }

progress{
  width: 100%;
}
.progress-box{
  display: flex;
  height: 30px;
}

